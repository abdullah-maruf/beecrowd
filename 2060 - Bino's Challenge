#include<stdio.h>
int main()
{
    int b,i,m2=0,m3=0,m4=0,m5=0;
    scanf("%d",&b);
    int a[b];
    for(i=0;i<b;i++){
        scanf("%d",&a[i]);
        if(a[i]%2==0){
            m2++;
        }
        if(a[i]%3==0){
            m3++;
        }
        if(a[i]%4==0){
            m4++;
        }
        if(a[i]%5==0){
            m5++;
        }
    }
    printf("%d Multiplo(s) de 2\n",m2);
    printf("%d Multiplo(s) de 3\n",m3);
    printf("%d Multiplo(s) de 4\n",m4);
    printf("%d Multiplo(s) de 5\n",m5);
    return 0;
}
